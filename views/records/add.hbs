<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<form method="post">
  <fieldset>
    <label for="albumTitle">Album Title:</label>
    <input type="text" id="albumTitle" name="albumTitle" required />
  </fieldset>
  <fieldset>
    <label for="albumArtist">Album Artist:</label>
    <input type="text" id="albumArtist" name="albumArtist" required />
  </fieldset>
  <fieldset>
    <label for="releaseYear">Release Year:</label>
    <input type="number" min="1900" max="2099" step="1" value="2016" id="releaseYear" name="releaseYear" required />
  </fieldset>
  <fieldset>
    <label for="noOfSongs">Number of Songs:</label>
    <input type="number" id="noOfSongs" name="noOfSongs" />
  </fieldset>
  <button type="submit">Add Record</button>
</form>
